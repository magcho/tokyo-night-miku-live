body
  background: #6D6A68
  margin: 0
header
  background: black
  width: 70%
  margin: 0 auto;
  position: relative;
  &:after
    content: ""

  #logo1,#logo2
    transition-duration: 800ms
    transition-timing-function: ease;
    position: absolute;
    // max-height: 60%
    width: 100%
    // height: 100%
    margin: auto 0


  #logo1
    z-index: 10
    opacity: 1
    #animation-svg
      position: relative;
      width: 100%
      height: auto;

  #logo2
    z-index: 0
    opacity: 0
    height: 100%
    position: absolute;
    left:50%
    top: 50%
    img
      display: block;
      width: 100%
      height: auto;
      transform: translateX(-50%)
